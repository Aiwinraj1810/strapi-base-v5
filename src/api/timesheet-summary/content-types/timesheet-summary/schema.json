{
  "kind": "collectionType",
  "collectionName": "timesheet_summaries",
  "info": {
    "singularName": "timesheet-summary",
    "pluralName": "timesheet-summaries",
    "displayName": "Timesheet Summary"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "weekStart": {
      "type": "date"
    },
    "weekEnd": {
      "type": "date"
    },
    "totalHours": {
      "type": "decimal"
    },
    "summaryStatus": {
      "type": "enumeration",
      "default": "Missing",
      "enum": [
        "Missing",
        "Incomplete",
        "Completed"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "timesheet_summaries"
    }
  }
}
